{
  "version": 2,
  "builds": [
    {
      "src": "/apps/backend/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/apps/frontend/src/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
